<?xml version="1.0" encoding="utf-8"?>
<resources>


    <style name="DigitTxtTitreStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">36dp</item>
        <item name="android:textColor">@color/brown_fonce</item>
        <item name="android:textSize">28sp</item>
        <item name="android:fontFamily">casual</item>
    </style>



    <style name="DigitTxtTableTitreStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAlignment">center</item>
        <item name="android:background">@drawable/cell_shape_color_brown</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">18dp</item>
        <item name="android:padding">10dp</item>
        <item name="android:textStyle">bold</item>

    </style>

    <style name="DigitTxtTableTitreStyle_MP">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAlignment">center</item>
        <item name="android:background">@drawable/cell_shape_color_brown</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16dp</item>
        <item name="android:padding">4dp</item>
        <item name="android:textStyle">bold</item>

    </style>

    <style name="TableColumStyle0">

        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/brown_fonce</item>
        <item name="android:textSize">14dp</item>
        <item name="android:textAlignment">center</item>

    </style>


    <style name="TableButonStyle">

        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/brown_fonce</item>
        <item name="android:textSize">14dp</item>

    </style>

    <style name="TableButonStyle_MP">

        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/brown_fonce</item>
        <item name="android:textSize">12dp</item>

    </style>

    <style name="DigitTxtStyle120">
        <item name="android:layout_width">120dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@color/brown_fonce</item>
        <item name="android:textSize">20dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:textAlignment">textEnd</item>
    </style>

    <style name="DigitTxtStyle20">
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@color/brown_fonce</item>
        <item name="android:textSize">22dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:textAlignment">textEnd</item>
    </style>


    <style name="DigitTxtEditerStyleStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/brown_fonce</item>
        <item name="android:textSize">22dp</item>
        <item name= "android:minHeight">48dp</item>


